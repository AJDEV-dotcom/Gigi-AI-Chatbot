{
  "expo": {
    "name": "gigi-chatbot",
    "slug": "gigi-chatbot",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon1.png",
    "scheme": "gigichatbot",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#212020",
        "foregroundImage": "./assets/images/icon1.png"
        // "backgroundImage": "./assets/images/android-icon-background.png",
        // "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anuuujjj.gigichatbot"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon1.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "64e6635e-0f11-44c4-9046-34f214575301"
      }
    },
    "fonts": [
      {
        "asset": "assets/fonts/PressStart2P-Regular.ttf",
        "family": "PressStart2P-Regular"
      }
    ]
  }
}
